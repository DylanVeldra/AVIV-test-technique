{
  "private": true,
  "version": "0.0.1",
  "main": "dist/index.js",
  "dependencies": {
    "axios": "^0.27.2",
    "express": "^4.17.3",
    "postgres": "^3.2.4"
  },
  "devDependencies": {
    "@jest/globals": "^29.4.2",
    "@types/express": "^4.17.14",
    "@types/jest": "^29.4.0",
    "@types/node": "^16.11.59",
    "concurrently": "^7.4.0",
    "jest": "^29.4.2",
    "msw": "^1.0.0",
    "nodemon": "^2.0.20",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "scripts": {
    "watch": "concurrently -k -n \"TypeScript,Node\" -p \"[{name}]\" -c \"blue,green\" \"npm run watch:typescript\" \"npm run watch:node\"",
    "watch:typescript": "tsc -b . --watch --preserveWatchOutput",
    "watch:node": "nodemon --delay 1 --inspect=0.0.0.0:9222 dist/index.js",
    "build": "tsc -b .",
    "start": "node dist/index.js",
    "test": "jest"
  }
}
